<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>Project</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>PersonController</servlet-name>
    <servlet-class>com.siwa.controller.PersonController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PersonController</servlet-name>
    <url-pattern>/PersonController.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ProjectController</servlet-name>
    <servlet-class>com.siwa.controller.ProjectController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ProjectController</servlet-name>
    <url-pattern>/ProjectController.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>CommentController</servlet-name>
    <servlet-class>com.siwa.controller.CommentController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CommentController</servlet-name>
    <url-pattern>/CommentController.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>TestController</servlet-name>
    <servlet-class>com.siwa.controller.TestController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TestController</servlet-name>
    <url-pattern>/TestController.do</url-pattern>
  </servlet-mapping>
  
</web-app>